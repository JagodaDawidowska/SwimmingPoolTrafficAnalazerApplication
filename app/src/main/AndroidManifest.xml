<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jdawidowska.myapplication3">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication3"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activites.SpaOneDayActivity"
            android:exported="false" />
        <activity
            android:name=".activites.PoolOneDayActivity"
            android:exported="false" />
        <activity
            android:name=".activites.CurrentDataActivity"
            android:exported="false" />
        <activity
            android:name=".activites.SpaOneHourActivity"
            android:exported="false" />
        <activity
            android:name=".activites.PoolOneHourActivity"
            android:exported="false" />
        <activity
            android:name=".activites.SpecifiedPeriodOfTimeActivity"
            android:exported="false" />
        <activity
            android:name=".activites.AllDataActivity"
            android:exported="false" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/network_security_config" />

        <activity
            android:name=".activites.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".example.AllDataLineChart"
            android:exported="false" />
    </application>

</manifest>